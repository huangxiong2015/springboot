package com.yikuyi.party.resource;

import com.yikuyi.party.contact.vo.MsgResultVo;

import feign.Headers;
import feign.Param;
import feign.RequestLine;

/**
 * 权限相关的接口调用定义
 * 
 * @author jik.shu@yikuyi.com
 */
@Headers({ "Content-Type: application/json;charset=UTF-8" })
public interface RegisterResource {
	
	/**
	 * 加入主账号
	 * @param entId
	 * @param account
	 * @return
	 * @since 2017年5月5日
	 * @author zr.aoxianbing@yikuyi.com
	 */
	@RequestLine("POST /v1/customer/join?entId={entId}&account={account}&applyId={applyId}")
//	@Headers({ "Authorization: Basic {authToken}" })
	public MsgResultVo joinMainAccount(@Param("entId") String entId, 
			@Param("account") String account,
			@Param("applyId") String applyId);
}