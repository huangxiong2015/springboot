package com.yikuyi.party.address.vo;

import java.io.Serializable;

import com.yikuyi.party.model.PartyContactMech.PurposeType;

import io.swagger.annotations.ApiModelProperty;

/**
 * 
 * @author zr.wuxiansheng@yikuyi.com
 * @version 1.0.0
 */
public class AddressVo implements Serializable{
	
	private static final long serialVersionUID = 6771634151016306310L;
	
	@ApiModelProperty(value = "地址ID")
	private String addressId;

	@ApiModelProperty(value = "联系人")
	private String toName;

	@ApiModelProperty(value = "联系手机")
	private String phoneNumber;

	@ApiModelProperty(value = "是否默认")
	private boolean defaultFlag;
	
	private BasicAddress addressInfo;
	
	@ApiModelProperty(value = "地址类型")
	private Currency currency;
	
	public enum Currency {
		/**
		 * 大陆(人民币)
		 */
		CNY,
		/**
		 * 香港(美元)
		 */
		USD	
	}
	
	@ApiModelProperty(value = "联系地址用途类型")
	private PurposeType purposeType;

	public String getAddressId() {
		return addressId;
	}

	public void setAddressId(String addressId) {
		this.addressId = addressId;
	}

	public String getToName() {
		return toName;
	}

	public void setToName(String toName) {
		this.toName = toName;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public boolean isDefaultFlag() {
		return defaultFlag;
	}

	public void setDefaultFlag(boolean defaultFlag) {
		this.defaultFlag = defaultFlag;
	}

	public BasicAddress getAddressInfo() {
		return addressInfo;
	}

	public void setAddressInfo(BasicAddress addressInfo) {
		this.addressInfo = addressInfo;
	}

	public Currency getCurrency() {
		return currency;
	}

	public void setCurrency(Currency currency) {
		this.currency = currency;
	}

	public PurposeType getPurposeType() {
		return purposeType;
	}

	public void setPurposeType(PurposeType purposeType) {
		this.purposeType = purposeType;
	}
	
	
}