<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yikuyi.party.vendor.dao.PartySupplierAliasDao">
   <resultMap id="BaseResultMap" type="com.yikuyi.party.vendor.vo.PartySupplierAlias">
		<id column="SUPPLIER_ALIAS_ID" property="supplierAliasId"/>
		<result column="PARTY_ID" property="partyId"/>
		<result column="ALIAS_NAME" property="aliasName"/>
		<result column="CREATED_DATE" property="createdDate" jdbcType="TIMESTAMP" />
		<result column="CREATOR" property="creator" jdbcType="VARCHAR" />
		<result column="LAST_UPDATE_USER" property="lastUpdateUser" jdbcType="VARCHAR" />
		<result column="LAST_UPDATE_DATE" property="lastUpdateDate" jdbcType="TIMESTAMP" />
  </resultMap>
  <insert id="insert" parameterType="com.yikuyi.party.vendor.vo.PartySupplierAlias">
		insert into PARTY_SUPPLIER_ALIAS
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="supplierAliasId != null">
				SUPPLIER_ALIAS_ID,
			</if>
			<if test="partyId != null">
				PARTY_ID,
			</if>
			<if test="aliasName != null">
				ALIAS_NAME,
			</if>
			
			<if test="creator != null">
				CREATOR,
			</if>
			<if test="createdDate != null">
				CREATED_DATE,
			</if>
			<if test="lastUpdateUser != null">
				LAST_UPDATE_USER,
			</if>
			<if test="lastUpdateDate != null">
				LAST_UPDATE_DATE,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="supplierAliasId != null">
				#{supplierAliasId,jdbcType=VARCHAR},
			</if>
			<if test="partyId != null">
				#{partyId,jdbcType=VARCHAR},
			</if>
			<if test="aliasName != null">
				#{aliasName,jdbcType=VARCHAR},
			</if>
			<if test="creator != null">
				#{creator,jdbcType=VARCHAR},
			</if>
			<if test="createdDate != null">
				#{createdDate,jdbcType=TIMESTAMP},
			</if>
			<if test="lastUpdateUser != null">
				#{lastUpdateUser,jdbcType=VARCHAR},
			</if>
			<if test="lastUpdateDate != null">
				#{lastUpdateDate,jdbcType=TIMESTAMP},
			</if>
		</trim>
	</insert>
	
	<update id="update" parameterType="com.yikuyi.party.vendor.vo.PartySupplierAlias" >
     update PARTY_SUPPLIER_ALIAS 
     <set >
      <if test="aliasName != null">
		 ALIAS_NAME = #{aliasName,jdbcType=VARCHAR},
		</if>
		 <if test="partyId != null">
		 PARTY_ID = #{partyId,jdbcType=VARCHAR},
		</if>
		<if test="lastUpdateUser != null">
			LAST_UPDATE_USER = #{lastUpdateUser,jdbcType=VARCHAR},
		</if>
		<if test="lastUpdateDate != null">
		  LAST_UPDATE_DATE = #{lastUpdateDate,jdbcType=TIMESTAMP},
		</if>
    </set>
       where SUPPLIER_ALIAS_ID = #{supplierAliasId,jdbcType=VARCHAR}
  </update>
  	<select id="isExistAliasName" resultMap="BaseResultMap">
      SELECT SUPPLIER_ALIAS_ID,PARTY_ID, ALIAS_NAME FROM PARTY_SUPPLIER_ALIAS WHERE LOWER(ALIAS_NAME) = LOWER(#{aliasName})
    </select>
    
    <select id="supperAliasNameList" resultMap="BaseResultMap">    
     SELECT SUPPLIER_ALIAS_ID,PARTY_ID, ALIAS_NAME,CREATOR, CREATED_DATE,LAST_UPDATE_USER,LAST_UPDATE_DATE FROM PARTY_SUPPLIER_ALIAS  WHERE 
     1=1 
     <if test="aliasName != null">
		AND LOWER(ALIAS_NAME) LIKE CONCAT('%',LOWER(#{aliasName}),'%')
	</if>
	 <if test="partyId != null">
		AND  PARTY_ID = #{partyId}
	</if>
    
   </select>
   <delete id="deleteSupperAlias">
      DELETE FROM PARTY_SUPPLIER_ALIAS WHERE SUPPLIER_ALIAS_ID=#{supplierAliasId}
	</delete>
    
</mapper>