  <select id="getPartyDetail" resultMap="BaseResultMap">
     SELECT t.PARTY_ID,t.`STATUS_ID`,t.CORPORATION_ID,t1.`GROUP_NAME`,t1.`LOGO_IMAGE_URL`,t1.VERIFY_STATUS_ID,t1.ACCOUNT_STATUS,t1.`COMMENTS`,t.PARTY_CODE FROM  `PARTY` t LEFT JOIN `PARTY_GROUP` t1 ON t.`PARTY_ID` = t1.`PARTY_ID` 
     where 1=1
       <if test="id != null and id != ''" >
        AND  t.PARTY_ID = #{id,jdbcType=VARCHAR}
       </if>
        <if test="partyType != null" >
          AND t.`PARTY_TYPE_ID`=#{partyType}
       </if>
  </select>
  
  <select id="getPartyPersonDetail" resultMap="BaseResultMap" parameterType="java.lang.String">
	SELECT 
	  t.PARTY_ID,
	  t.`STATUS_ID`,
	  t1.`LAST_NAME_LOCAL`,
	  t.`CORPORATION_ID`,
	  t.PARTY_TYPE_ID,
	  IFNULL(t1.`ACCOUNT_TYPE`,'COMMON') as ACCOUNT_TYPE
	FROM
	  `PARTY` t 
	  LEFT JOIN `PERSON` t1 
	    ON t.`PARTY_ID` = t1.`PARTY_ID` 
	    AND t.`PARTY_TYPE_ID` = 'PERSON' 
	WHERE t.PARTY_ID = #{id}
  </select>
  
