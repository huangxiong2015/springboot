package com.yikuyi.party.model;




import java.util.Date;

import com.ykyframework.model.AbstractEntity;

import io.swagger.annotations.ApiModelProperty;

/**
 * 
 * @author zr.aoxianbing@yikuyi.com
 * @version 1.0.0
 */
public class PartyStatusHis extends AbstractEntity{

	
	
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 7371121648828049520L;
	@ApiModelProperty(value = "状态ID")
	private String id;
	@ApiModelProperty(value = "状态日期", example="2016-01-01 08:00:00")
	private Date statusDate;

	private Status status;
	public enum Status{
		
		/**
		 *  未激活
		 */
		WAIT_ACTIVE,
		/**
		 *  未关联活
		 */
		WAIT_RELATION,
		/**
		 *  激活审核
		 */
		WAIT_ACTIVE_IMPROVE,
		/**
		 *  激活驳回
		 */
		ACTIVE_REJECT,
		/**
		 *  关联审核
		 */
		WAIT_RELA_IMPROVE,
		/**
		 *  关联驳回
		 */
		RELATION_REJECT,
		/**
		 *  已关联
		 */
		RELATED,
		/**
		 *  已激活
		 */
		ACTIVE
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public Date getStatusDate() {
		return statusDate;
	}

	public void setStatusDate(Date statusDate) {
		this.statusDate = statusDate;
	}

	public Status getStatus() {
		return status;
	}

	public void setStatus(Status status) {
		this.status = status;
	}


}
