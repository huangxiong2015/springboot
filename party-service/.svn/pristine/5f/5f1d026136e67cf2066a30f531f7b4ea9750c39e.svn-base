	public String updatePwd(String userId,String passWord,String partyId) {
		String flag;
		String jpartyId = null;
		try {
			byte[] result = Base64.decodeBase64(partyId);
			jpartyId = new String(result);
		} catch (Exception e) {
			logger.error(partyId+",account base64解密 error:{}",e);
