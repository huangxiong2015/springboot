package com.yikuyi.party;

import com.yikuyi.party.config.BaseClient;
import com.yikuyi.party.resource.ACLResource;
import com.yikuyi.party.resource.ContactMechResource;
import com.yikuyi.party.resource.DeptResource;
import com.yikuyi.party.resource.EnterpriseResource;
import com.yikuyi.party.resource.FindPasswordResource;
import com.yikuyi.party.resource.PartyAddressResource;
import com.yikuyi.party.resource.PartyGroupResource;
import com.yikuyi.party.resource.PersonResource;
import com.yikuyi.party.resource.RegisterResource;
import com.yikuyi.party.resource.UserLoginResource;
import com.yikuyi.party.resource.UserResource;

public class PartyClient extends BaseClient {

	public PartyClient(String baseUrl) {
		this.setBaseUrl(baseUrl);
	}

	public ACLResource aclResource() {
		return super.builder(ACLResource.class);
	}

	public ContactMechResource contactMechResource() {
		return super.builder(ContactMechResource.class);
	}

	/**
	 * 适用方法返回参数是复杂对象
	 * @return
	 */
	public PersonResource personResource() {
		return super.builder(PersonResource.class);
	}
	
	/**
	 * 适用方法返回参数是String对象
	 * @param javaClass
	 * @return
	 */
	public PersonResource personResource(Class<String> javaClass) {
		return super.builder(PersonResource.class,javaClass);
	}

	public PartyAddressResource partyAddressResource() {
		return super.builder(PartyAddressResource.class);
	}

	public UserLoginResource userLoginResource() {
		return super.builder(UserLoginResource.class);
	}

	public RegisterResource registerResource() {
		return super.builder(RegisterResource.class);
	}

	public FindPasswordResource findPasswordResource() {
		return super.builder(FindPasswordResource.class);
	}

	public PartyGroupResource partyGroupResource() {
		return super.builder(PartyGroupResource.class);
	}

	public EnterpriseResource enterpriseResource() {
		return super.builder(EnterpriseResource.class);
	}

	public UserResource userResource() {
		return super.builder(UserResource.class);
	}
	
	public UserResource userResource(Class<String> javaClass) {
		return super.builder(UserResource.class,javaClass);
	}

	public DeptResource deptResource() {
		return super.builder(DeptResource.class);
	}
}