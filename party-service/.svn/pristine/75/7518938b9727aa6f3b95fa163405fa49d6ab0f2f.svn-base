spring.profiles.active=dev
#server.context-path=/order
logging.config=classpath:config/logback.xml

spring.datasource.type=com.alibaba.druid.pool.DruidDataSource
spring.datasource.driver-class-name=com.mysql.jdbc.Driver
#spring.datasource.maximum-pool-size=100
spring.datasource.maxActive=20
#spring.datasource.max-idle=10
spring.datasource.max-wait=10000
spring.datasource.min-idle=5
spring.datasource.initial-size=5
spring.datasource.validation-query=SELECT 1
spring.datasource.test-on-borrow=false
spring.datasource.test-while-idle=true
spring.datasource.time-between-eviction-runs-millis=18800
# \u6253\u5f00PSCache\uff0c\u5e76\u4e14\u6307\u5b9a\u6bcf\u4e2a\u8fde\u63a5\u4e0aPSCache\u7684\u5927\u5c0f 
spring.datasource.poolPreparedStatements=true
spring.datasource.maxPoolPreparedStatementPerConnectionSize=20
# \u914d\u7f6e\u76d1\u63a7\u7edf\u8ba1\u62e6\u622a\u7684filters\uff0c\u53bb\u6389\u540e\u76d1\u63a7\u754c\u9762sql\u65e0\u6cd5\u7edf\u8ba1\uff0c'wall'\u7528\u4e8e\u9632\u706b\u5899 
spring.datasource.filters=stat,wall,slf4j
# \u901a\u8fc7connectProperties\u5c5e\u6027\u6765\u6253\u5f00mergeSql\u529f\u80fd\uff1b\u6162SQL\u8bb0\u5f55
#spring.datasource.connectionProperties=druid.stat.mergeSql=true;druid.stat.slowSqlMillis=5000
spring.datasource.connectionProperties=druid.stat.mergeSql=true;druid.stat.logSlowSql=true;druid.stat.slowSqlMillis=5000
# \u5408\u5e76\u591a\u4e2aDruidDataSource\u7684\u76d1\u63a7\u6570\u636e
#spring.datasource.useGlobalDataSourceStat=true
# \u81ea\u5b9a\u4e49\u914d\u7f6e\uff0c\u662f\u5426\u6253\u5f00druid\u7684\u76d1\u89c6\u754c\u9762
spring.datasource.druid.statwatch=true

#spring.jackson.default-property-inclusion=non_null
spring.jackson.serialization.indent_output=true
spring.jackson.date-format=yyyy-MM-dd
spring.jackson.time-zone=Asia/Shanghai

#mybatis config
mybatis.config-location=classpath:config/mybatis/mybatis-config.xml
mybatis.mapper-locations=classpath*:/dao/*Dao.xml

#redisCache keyName
cache.config.redis.cacheNames.usernameCache=200
cache.config.redis.cacheNames.userShiroAuthorityInfoCache=10
cache.config.redis.cacheNames.sendActiveMailCache=86400
#\u6240\u6709\u4f9b\u5e94\u5546\u548c\u7269\u6d41
cache.config.redis.cacheNames.partyGroupGetAllPartyGroupListCache=300
#\u6240\u6709\u4e2a\u4eba\u7528\u6237
cache.config.redis.cacheNames.allUserInfoCache=300

#mq topicName config
mqConsumeConfig.sendMsgAndEmail.topicName=TOPIC_MSG_EMAIL
mqConsumeConfig.sendAudit.topicName=TOPIC_MSG_AUDIT
mqConsumeConfig.inquiryCustomerUpdate.topicName=TOPIC_InquiryCustomerUpdate_Notify

mq.config.mqConsumeConfig.auditLogSub = {"consumerGroup":"CID_YKY_AuditLog","topicName":"TOPIC_MSG_AUDIT",concurrency:"1","msgReceiveListener":"com.yikuyi.party.listener.AuditLogReceiveListener"}

#\u7269\u7406topic\u540d\uff0c\u53ef\u5728\u6620\u5c04\u4e2d\u914d\u7f6e\u4f5c\u4e3a\u4e0a\u8fb9topic\u7684\u5b9e\u9645\u53d1\u9001\u5230\u7684topic\uff0c\u505a\u8bbe\u7f6e\u4f7f\u7528\uff0c\u4e0d\u80fd\u5728\u4ee3\u7801\u4e2d\u4f7f\u7528
#messages & email & in_mail, notifies for application ...
mq.topic.target.original.MESSAGES=TOPIC_ORIGINAL_MESSAGES
#product & stock
mq.topic.target.original.PRODUCTS=TOPIC_ORIGINAL_PRODUCTS
#order & payment
mq.topic.target.original.ORDER_PAY=TOPIC_ORIGINAL_ORDER_PAY
#discard
#system
mq.topic.target.original.SYSTEM=TOPIC_ORIGINAL_SYSTEM

mq.config.mqTopicMappingConfig = {"TOPIC_InquiryCustomerUpdate_Notify":"${mq.topic.target.original.MESSAGES}","TOPIC_MSG_EMAIL":"${mq.topic.target.original.MESSAGES}","TOPIC_MSG_AUDIT":"${mq.topic.target.original.SYSTEM}","TOPIC_PARSE_IMPORT_FILE":"${mq.topic.target.original.PRODUCTS}","TOPIC_CANCEL_ORDER":"${mq.topic.target.original.ORDER_PAY}","TOPIC_CREATE_PRODUCT":"${mq.topic.target.original.PRODUCTS}","TOPIC_SYNC_ELASTICSEARCH_PRODUCT":"${mq.topic.target.original.PRODUCTS}","TOPIC_PaymentSent_Notify":"${mq.topic.target.original.ORDER_PAY}","TOPIC_PaymentResult_Notify":"${mq.topic.target.original.ORDER_PAY}","TOPIC_UpdateOrderPaymethod_Notify":"${mq.topic.target.original.ORDER_PAY}"}

mq.producer.groupid=PID_ICTRADE_PRODUCER
