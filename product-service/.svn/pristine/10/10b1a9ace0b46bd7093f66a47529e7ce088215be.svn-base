package com.yikuyi.promotion.vo;

import io.swagger.annotations.ApiModelProperty;

public class PromotionPreviewVo {
 
	@ApiModelProperty(value="图片路径")
	private String imageUrl;
	
	@ApiModelProperty(value="活动细则")
	private String showRule;
	
	@ApiModelProperty(value="细则详情")
	private String ruleContent;

	
	public String getShowRule() {
		return showRule;
	}

	public void setShowRule(String showRule) {
		this.showRule = showRule;
	}


	public String getImageUrl() {
		return imageUrl;
	}

	public void setImageUrl(String imageUrl) {
		this.imageUrl = imageUrl;
	}


	public String getRuleContent() {
		return ruleContent;
	}

	public void setRuleContent(String ruleContent) {
		this.ruleContent = ruleContent;
	}
}
