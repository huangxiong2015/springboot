a:hover, a:active, a:focus{ 
	color: #72afd2;
}
.selector_box{padding:20px;}
.search_criteria a {
        font-size:12px;
        display: inline-block;
        width: 18px;
        height: 18px;
        line-height: 18px;
        border: 1px solid #e4e4e4;
        margin-right: 5px;
        text-align: center;
        float: left;
        margin-top: 3px;
        color: #666;
    }
    .search_criteria .active {
        color: #b1191a;
        border: 1px solid #b1191a;
        cursor: pointer;
    }
    .search_criteria .disable {
        color: #d6d6d6;
        cursor: default;
    }
    .supplier_list{
      overflow: auto;
      max-height: 110px;
      margin-top: 20px;
    }
    .supplier_list span {
        font-size:12px;
        display: inline-block;
        width: 150px;
        margin-right: 30px;
        margin-bottom: 13px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        float: left;
        height: 16px;
        line-height: 16px;
    }
  .supplier_list a.active {
        color: #b1191a;
    }
