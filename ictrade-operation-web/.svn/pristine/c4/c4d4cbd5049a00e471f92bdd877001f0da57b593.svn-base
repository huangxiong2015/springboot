	padding: 20px 0;
}

.coupon-upload li .coupon-bread{
	display: inline-block;
	padding: 0 15px;
	border: 1px solid #C11F2E;
	margin:0 5px 5px;
}

.coupon-upload li:last-child{
	border-bottom: none;
}

.coupon-edit{
	z-index: 5;
	cursor: pointer;
}

#file-upload{
	margin: 30px;
}

#file-upload .form-label{
	font-size: 12px;
	font-weight: normal;
	width: 100px;
	padding: 6px 0;
	float:left;
}

#file-upload .form-control{
	height: auto;
	border: none;
	padding-left: 120px;
}
#file-upload .form-control span{
	position:relative;
	margin-right: 30px;
	display:inline-block;
}
#file-upload .form-control input{
	width: 96px;
	height: 32px;
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0;
	
}
#file-upload .form-control .tip{
	margin-top: 20px;
	color: rgb(254,92,31);
}
