	.search_box{
		position: relative;
	    border: 1px solid #B61D1D;
		float: left;
		width: 450px;
		height: 36px;
		margin-top: 13px;
		margin-left: 440px;
		font-size: 12px;
	}
	.select_way{
		width: 64px;
		height: 34px;
		line-height: 34px;
		text-align: center;
		background-color: #f9f9f9;
		border:1px solid #e4e4e4;
		color: #666;
	}
	.button_search{
		position: relative;
	    float: right;
	    /*margin-right: 20px;*/
	    margin-top: -36px;
	    width: 80px;
	    height: 36px;
	    line-height: 36px;
	    text-align: center;
	    color: #fff;
	    background-color: #b2100d;
	    border: none;
	    cursor: pointer;
	}
	.search_box .search_text{
	    position: absolute;
	    padding-left: 10px;
	    width: 294px;
	    height: 34px;
	    border:1px solid #e4e4e4;
	    color: #333333;
	    outline: 0;
	    top: 0;
	    margin-left: 65px;
	    font-size: 14px;
	    display: block;
	    line-height: 34px;
	}
