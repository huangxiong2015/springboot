.input-content {
  float: left;
  line-height: 30px;
}
.input-text {
  margin-right: 10px;
  width: 130px;
  text-align: right;
  color: #666;
}
.input-text .must {
  margin-right: 5px;
  color: #b1191a;
}
.ui-qual .input-content {
  color: #999;
  *zoom: 1;
}
.ui-qual .input-content:after {
  display: block;
  content: '';
  clear: both;
}
.uploadImg {
  position: relative;
  float: left;
  width: 163px;
  height: 108px;
  background: url(../../images/zizi-default.png) no-repeat center 20px;
}
.uploadImg img {
  width: 163px;
  height: 108px;
  /* background: #fff; */
}
.uploadImg img[data-type=pdf] {
	width: 72px;
	height: auto;
}
.uploadImg .upload-btn {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 32px;
  line-height: 32px;
  border-radius: 0;
  font-size: 14px;
  color: #fff;
  background: #666;
}
.uploadCz {
  position: relative;
  float: left;
  margin-left: 15px;
  height: 110px;
}
.uploadCz .tips {
  line-height: 22px;
}
.uploadCz .ck-btn {
  position: absolute;
  bottom: 0;
  width: 64px;
  height: 22px;
  line-height: 20px;
  color: #999;
  border: 1px solid #e4e4e4;
  border-radius: 3px;
  background: #fff;
  font-size: 12px;
  cursor: pointer;
  text-align: center;
}
table {
	font-size: 14px;
	text-align: center;
}
.infoFrom {
	position: relative;
}
.verify-status {
	position: absolute;
	right: 50px;
	padding: 8px 24px;
	border-radius: 0;
}
.table>tbody>tr>td {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}