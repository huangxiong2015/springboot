@CHARSET "UTF-8";
.lh32{line-height:32px;}
.mt20{margin-top:20px;}
.pdl15{padding-left:15px;}
.select_image{display: inline-block;padding: 6px 17px;cursor: pointer;border: solid 1px #ddd;}
.return_btn{padding:10px 20px;cursor: pointer;border: solid 1px #ddd;border-radius: 3px;font-size:14px;}
.return_btn:hover{color:#b1191a;border:solid 1px #b1191a;}
.c_tip{background:#fff;border:solid 1px #ddd;color:#666;text-align: center;}
.b_red{border:solid 1px #b1191a !important;}
.ui-state-disabled{opacity:1;background:#eee !important;}
.condition_inquery{background-color: #c11f2e;color: #fff;padding: 10px 20px;font-size: 14px;border-radius: 3px;}
.condition_inquery:hover, .condition_inquery:active, .condition_inquery:focus{color:#fff;}
.return_btn{padding: 10px 20px;cursor: pointer;border: solid 1px #ddd;border-radius: 3px;font-size: 14px;color:#919191;margin-left:30px;}
.return_btn:hover{color: #b1191a;border: solid 1px #b1191a;}
.dn{display:none;}
.l{float:left;} 
.logo-box img{width:100px;height:100px;}
.selector_box{padding:20px;} 
.search_criteria a {
        font-size:12px;
        display: inline-block;
        width: 18px;
        height: 18px;
        line-height: 18px;
        border: 1px solid #e4e4e4;
        margin-right: 5px;
        text-align: center;
        float: left;
        margin-top: 3px;
        color: #666;
    }
    .search_criteria .active {
        color: #b1191a;
        border: 1px solid #b1191a;
        cursor: pointer;
    }
    .search_criteria .disable {
        color: #d6d6d6;
        cursor: default;
    }
    .supplier_list{
      overflow: auto;
      max-height: 110px;
      margin-top: 10px;
    }
    .supplier_list span {
        font-size:12px;
        display: inline-block;
        width: 150px;
        margin-right: 30px;
        margin-bottom: 13px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        float: left;
        height: 16px;
        line-height: 16px;
    }
  .supplier_list a.active {
        color: #b1191a;
    }
  .supplier_list .mr0{margin-right:0px;}
  .s_select_data{
    max-height: 320px;
  } 
   .search_box {
    width:151px;
    height:24px;
    line-height:24px;
    padding-left:20px;
  }
  .distributor{
    display:block!important;
   }
  .manufacturer{
    display:block!important;
   }
   .other{
        display: inline!important;
   }
   .logo-box #uploadback,.logo-box .uploadback-btn{
    position: absolute;
    bottom: 0;
    width: 100px;
    filter: alpha(opacity=0.8);
    -moz-opacity: .8;
    -khtml-opacity: .8;
    opacity: .8;
   }
   .character_tip{
   	position: absolute;
    right: -90px;
    top: 6px;
    color: #dd4b39;
   }
   .layui-layer-content{
     padding:10px;
   }
   .error{
	border:1px  solid #b11919 !important;
  }
/* 选择供应商 */
.letter-select-wrap{
	padding-top:7px;
}
.letter-select{
	padding-left: 20px;
	padding-right:20px;
    overflow-x: hidden;
}
.letter-select-wrap .form-group{
	margin-left:0;
	margin-right:0;
}
.ml5{
	margin-left:5px;
}
.advertisement-edit{
	cursor:pointer;
}
/* 组件已选择样式 */
.selected-list .s-selected,.letter-select-wrap .s-selected{
    display: inline-block;
    margin: 0 10px 10px 0;
    padding: 0 9px 0 11px;
    height: 28px;
    line-height: 28px;
    border: 1px dashed #ddd;
    color: #333;
    position: relative;
    font-weight: normal;
    cursor:pointer;
}
.selected-list .s-selected .icon-close-min,.letter-select-wrap .s-selected .icon-close-min{
	margin-left:10px;
}
.selected-list .s-selected:hover,.letter-select-wrap .s-selected:hover{
    color: #b1191a;
    border-color: #b1191a;
}
.letter-select-wrap .search-box input{
	line-height:30px;
	height:30px;
}
.letter-select-wrap .pinyin-t span a{
	margin: 0 2px 5px;
	line-height:22px;
}
/* 详情页 */
.group-name{
	/* margin-right:5px; */
}
/* 分类广告 */
.img-desc{
	padding-top:7px;
}
.image-url-input{
	margin-top:101px;
}
/* 广告小图 */
.small-ad-list{
	padding-left:15px;
}
.small-ad-list li{
	position:relative;
	width:100px;
	height:100px;
	display:inline-block;
	margin-right: 20px;
    margin-bottom: 20px;
}
.small-ad-input-list .image-url-input{
	margin-top:10px;
}
.small-ad-input-list{
	padding-left:0;
}
.logo-box .i-delete{
	display:none;
    position: absolute;
    right: 0;
    top: 0;
    font-size: 14px;
    padding: 4px;
    cursor: pointer;
}
.logo-box:hover .i-delete{
	display:block;
}
.logo-box:hover .i-delete:hover {
    background: #b1191a;
    color: #fff;
}
.big-ad-list .logo-box{
	width:290px;
}
.big-ad-list img{
	height:135px;
	width:290px;
}
.big-ad-list .logo-box .uploadback-btn{
	width:290px;
}

/* 分类广告详情 */
.big-ad-list.detail-list{
	padding-left:0;
}
.big-ad-list.detail-list li{
	margin-bottom:20px;
}
.small-url-list p,.big-ad-list p{
	width: 100%;
    height: auto;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
}
.poi{
	cursor:pointer;
}
.pdt7{
	padding-top:7px;
}