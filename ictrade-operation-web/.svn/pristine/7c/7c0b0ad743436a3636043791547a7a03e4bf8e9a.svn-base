/* 保存成功提示框 start*/
.infoSuc {position: fixed;left: 0;right: 0;top: 0;bottom: 0;margin: auto;width: 506px;height: 131px;background-color: #fff;line-height: 131px;border: 3px solid #afafaf;z-index:9999999;}
.infoSuc .icon-right-c {display: inline-block;margin-right: 22px;color: #87B848;font-size: 22px;font-weight: bold;vertical-align: -2px;}

.cRed {
	display: inline-block;
	margin-right: 6px;
	color: #b1191a;
	font-size: 16px;
	vertical-align: -3px;
}
.input-title {
	position: relative;
	margin-bottom: 4px;
	height: 48px;
	line-height: 48px;
	color: #666;
	font-size: 14px;
}

.input-title:before {
	content: '';
	display: inline-block;
	margin-right: 5px;
	width: 3px;
	height: 15px;
	vertical-align: -3px;
	background: #d48081;
}

