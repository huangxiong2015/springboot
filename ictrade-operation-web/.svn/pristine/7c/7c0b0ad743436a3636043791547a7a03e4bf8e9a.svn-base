    width: 110px;
}
.check-box-white {
    background: url("../../images/sprite.png") no-repeat 0 -400px;
    width: 13px;
    height: 13px;
    display: inline-block;
    float: left;
    /*margin-top: 7px;*/
    overflow: hidden;
}
.check-box-red {
    background: url("../../images/sprite.png") no-repeat -20px -400px;
    width: 13px;
    height: 13px;
}
/*验证样式*/
em[class*='error'], i[class*='error'] {
	margin-top: 0px;
	margin-left: 20px;
    padding: 10px 12px 10px 12px;
    max-width: 320px;
    box-sizing: border-box;
    background-color: #fff4d7;
    font-size: 12px;
    color: #666;
}
em[class*='error']:before, i[class*='error']:before {
	position: relative;
    top: 3px;
    border: solid transparent;
    margin-left: -25px;
    width: 0;
    font-size: 19px;
    border-right-color: #fff4d7;
    color: #fff4d7;
}
.icon-confirm {
	/* position: absolute;
