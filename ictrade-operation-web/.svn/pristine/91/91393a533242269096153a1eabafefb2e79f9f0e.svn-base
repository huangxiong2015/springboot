/* 表单输入非法提醒样式 */
em[class*='error'] {
	position: absolute;
	top: 1px;
	display: inline-block;
	display:none;
	padding: 9px 10px;
	box-sizing: border-box;
	background-color: #fff4d7;
	font-size: 12px;
	color: #666;  /*transition:top 0.3s;*/
	margin-left:15px;
}
em[class*='error'].st2{
	padding-top: 2px;
    padding-bottom: 2px;
    top: 0;
}
em[class*='error']:before {
	border: solid transparent;
	content: ' ';
	height: 0;
	right: 100%;
	position: absolute;
	width: 0;
	border-width: 5px;
	border-right-color: #fff4d7;
	top: 60%;
	border-width: 8px;
	margin-top: -11px;
}
