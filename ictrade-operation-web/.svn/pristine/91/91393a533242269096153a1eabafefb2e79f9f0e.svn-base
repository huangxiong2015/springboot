.y_btn{
	display:inline-block;
	width:100px;
	height:31px;
	line-height: 31px;
	background:#c11f2d;
	color:#fff;
	text-align:center;
	font-size: 12px;
	cursor: pointer;
	border-radius:3px;
}
.y_btn:hover{
	background: #b1191a;
}
.y_btn:active{
	background: #c81724;
}
.r_btn{
	position: relative;
    top: 1;
	display:inline-block;
	width:99px;
	height:31px;
	line-height: 31px;
	margin-left:13px;
	text-align:center;
	background:#ffffff;
	border:1px solid #979797;
	cursor: pointer;
	border-radius:3px;
}
.r_btn:link,.r_btn:visited{
	color:#919191;
	text-decoration:none;
}
.r_btn:hover,.r_btn:active{
	color:#b1191a;
	border:1px solid #b1191a;
	text-decoration:none;
}

.up_btn{
	display: inline-block;
    position: relative;
    width: 80px;
    height: 35px;
    line-height: 35px;
    text-align: center;
    border: 1px solid #ddd;
    color: #333;
    cursor:pointer;
}
.up_file{
	position: absolute;
    width: 80px;
    height: 35px;
    line-height: 35px;
    text-align: center;
    left:0;
    top:0;
    z-index:3;
    opacity:0;
    cursor: pointer;
}
