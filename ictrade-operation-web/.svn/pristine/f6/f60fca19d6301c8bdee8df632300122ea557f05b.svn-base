<style type="text/css">
.condition_inquery {
	background-color: #c11f2e;
	color: #fff;
	padding: 10px;
	font-size: 14px;
	border-radius: 3px;
}

.col_bt_time input {
	width: 37%;
}

.col_bt label, .col_bt_time label {
	text-align: right;
}

.col_input_2 {
	width: 69%;
}

.col_input_3 {
	width: 59%;
}

.col_input_4 {
	width: 49%;
}

.mt25 {
	margin-top: 25px;
}

.content-wrapper .content-header .pitch_tab {
	border-top: 0;
}

.qualifications .dd2 span.dib {
	width: 100%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	margin: 14px 10px 0 -10px;
}

.ms-controller {
	visibility: hidden;
}

.qualifications .dd2 img.pdf_icon {
	border: none;
	margin-top: 10px;
	width: 32px;
	height: 32px;
}

.cur {
	cursor: pointer;
}

.table tr, .table td, .table th {
	border-left: none;
	border-right: none;
}

.qualifications .status {
	background: green;
	text-align: center;
	padding: 0 20px;
	color: #fff;
}
.col-xs-1, .col-xs-10, .col-xs-11, .col-xs-12, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, .col-xs-8, .col-xs-9 {
	float: none;
}
.sz-wrap {
	border: 1px solid #e4e4e4;
	border-top: none;
}
.general {
	width: auto;
	margin-left: 0;
	border-left: none;
	border-right: none;
}
#blank_jl_body, #blank_jl,#blank_jl td, #blank_jl_body2, #blank_jl2, #blank_jl2 td{
	border: none;
}
#blank_jl td, #blank_jl2 td {
	padding-top: 30px;
}
.info_new {
	margin: 0;
}
.qualifications  .dd2 + .dd2 {
	width: 200px;
}
.info_new .item, .info_new label {
	line-height: 30px;
}
</style>
