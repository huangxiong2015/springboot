    <!-- Main content -->
	<section class="content">
		<div class="panel panel-default">
		  <div class="panel-heading">
		    <h3 class="panel-title">创建代金券</h3>
		  </div>
		  <div class="panel-body">
		  	<!-- main_cont-->
		  	<div class="main_cont">
		  		<!-- main_cont_l -->
			    <div class="main_cont_l">
			    	<h4 class="list_title">
			    		代金券基本属性
			    	</h4>
			    	<ul class="list-group">
					  <li class="item">
					  	<span class="item_label"><i class="r_tag">*</i>代金券类型：</span>
					  	<div class="item_r">
					  		<span  class="c_type_wrap">
					  			<select name="cType" id="cType">
						  			<option value="">平台推广劵</option>
						  			<option value="">平台定向劵</option>
						  			<option value="">地推劵</option>
						  		</select>
					  		</span>
					  	</div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>代金券名称：</span>
						 <div class="item_r">
							<input type="text" name="cName" class="s_input" placeholder="" maxlength="50"   ms-duplex="">
						 </div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>有效时间：</span>
						 <div class="item_r">
							<p class="effect_type_wrap">
								<select name="cEffectTimeType" id="cEffectTimeType">
									<option value="">限时使用</option>
									<option value="">限时领取</option>
								</select>
						 	</p>
							<p class="mt10">
						 		<span class="item_label"></span>
						 		<span class="rel">
					                <input id="applyBegin" value="" name="applyBegin" data-duplex="@parameter.applyBegin" ms-attr="{value:@parameter.applyBegin}" class="main_head_input " type="text">
					                <span class="cc1 icon-calendar"></span>
				                </span>
				                <span style="color:#aaa; margin-left:5px;margin-right:5px;">-</span>
				                <span class="rel">
					                <input id="applyEnd" value="" name="applyEnd" data-duplex="@parameter.applyEnd" ms-attr="{value:@parameter.applyEnd}" class="main_head_input" type="text">
					                <span class="cc2 icon-calendar"></span>
				                </span>
						 	</p>
						 </div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>总发行量：</span>
						 <div class="item_r">
							<div>
								<input type="text" name="cTotal" class="s_input " placeholder="" maxlength="50"  ms-duplex="">
								<span>张</span>
							</div>
						 </div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>单张面值：</span>
						 <div class="item_r">
							<input type="text" name="cValue" class="s_input " placeholder="" maxlength="50"  ms-duplex="">
						 	<span class="c_wrap">
						 		<select name="cCurrency" id="cCurrency">
							 		<option value="">RMB</option>
							 		<option value="">USD</option>
							 	</select>
						 	</span>
						 </div>
					  </li>
					</ul>
					
					<h4 class="list_title">
			    		使用条件
			    	</h4>
			    	<ul class="list-group">
					  <li class="item">
					  	<span class="item_label"><i class="r_tag">*</i>可用货品：</span>
					  	<div class="item_r">
					  		<p><span class="chose_t">选择货品</span></p>
					  	</div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>使用限额：</span>
						 <div class="item_r">
							<span>单笔订单中此优惠券指定货品金额满</span>
							<span class="ml5"><input type="text" name="cName" class="s_input" placeholder="" maxlength="50"  style="width:80px;" ms-duplex=""></span>
							<span class="currency">RMB</span>
							<span>使用</span>
						 </div>
					  </li>  
					</ul>
					
					<h4 class="list_title">
			    		使用条件
			    	</h4>
			    	<ul class="list-group">
					  <li class="item">
					  	<span class="item_label"><i class="r_tag">*</i>领取条件：</span>
					  	<div class="item_r">
					  		<p><input type="radio" /><label for="">自助领取</label></p>
					  	</div>
					  </li>
					  <li class="item">
					  	 <span class="item_label"><i class="r_tag">*</i>推广对象：</span>
						 <div class="item_r">
							<p>
							<span><input type="checkbox" name="" value="" /><label for="">企业用户</label></span>
							<span class="ml5"><input type="checkbox" name="" value="" /><label for="">个人用户</label></span>
							</p>
						 </div>
					  </li>  
					</ul>
					<h4 class="list_title">
			    		其他
			    	</h4>
