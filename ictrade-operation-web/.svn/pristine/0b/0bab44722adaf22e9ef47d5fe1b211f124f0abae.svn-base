/* 模块维护编辑页面 */
.return_btn{padding:10px 20px;cursor: pointer;border: solid 1px #ddd;border-radius: 3px;font-size:14px;}
.return_btn:hover{color:#b1191a;border:solid 1px #b1191a;}
.ueditor_wrapper{
	margin:0 auto;
	overflow:hidden;
	border:0px;
	text-align: left;
}
