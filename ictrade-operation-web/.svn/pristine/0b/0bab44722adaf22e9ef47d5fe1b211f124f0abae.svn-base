/* 模块维护树型页面 */
.ms-controller{visibility: hidden;}
.ztree *{font-family:'Microsoft Yahei', Arial, Simsun, Helvetica, sans-serif;}
th{font-weight:normal;background:#f2f2f2;}
th,td{text-align:center;border:1px solid #d2d2d2;height:46px;line-height:46px;}
a:hover{color:#b1191a;}
.m_edit_icon{display:inline-block;width:16px;height:16px;background: url(../../images/edit_delete_sprite.png) no-repeat 0px 0px;cursor: pointer;}
.m_delete_icon{display:inline-block;width:16px;height:16px;background: url(../../images/edit_delete_sprite.png) no-repeat -15px 0px;cursor: pointer;}
.m_preview_icon{display:inline-block;width:16px;height:16px;background: url(../../images/edit_delete_sprite.png) no-repeat 0px -14px;cursor: pointer;}
.g_blue,.g_blue:hover{color:#3377aa;cursor:pointer;}
