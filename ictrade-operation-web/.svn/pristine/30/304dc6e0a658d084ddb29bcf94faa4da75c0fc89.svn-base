@CHARSET "UTF-8";
.text-r{
	text-align:right;
}
.seo-textarea{
	min-height:50px;
	min-width:470px;
}
.seo-wrap{
	border:solid 1px #d2d6de;
	padding:5px 15px;
	margin-left:15px;
}
.page-title{
	font-size: 16px;
	margin: 0;
	color: #333;
}

.time-tab{
	margin: 0;
	overflow: hidden;
	padding: 0;
}

.time-tab li {
	float: left;
	margin-right: 15px;
	list-style: none;
	margin-top: 10px;
}

.time-tab li a{
	display: inline-block;
	border: 1px solid #ccc;
    border-radius: 3px;
    padding: 5px 10px;
}

.time-tab li a:hover,.time-tab li.active a{
	background: rgb(254,92,31);
	border: 1px solid rgb(254,92,31);
	color: #fff;
}

.add-product{
	width: auto;
}

#file-upload{
	margin: 30px;
}

#file-upload .form-label{
	font-size: 12px;
	font-weight: normal;
	width: 100px;
	padding: 6px 0;
	float:left;
}

#file-upload .form-control{
	height: auto;
	border: none;
	padding-left: 120px;
}
#file-upload .form-control span{
	position:relative;
	margin-right: 30px;
	display:inline-block;
}
#file-upload .form-control input{
	width: 96px;
	height: 32px;
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0;
	
}
#file-upload .form-control .tip{
	margin-top: 20px;
	color: rgb(254,92,31);
}
.activity-downlad{
	overflow: hidden;
	padding: 0 20px 0;
	position: relative;
    top: -14px;
    margin-left: 65px;
}
.activity-downlad a{
	margin-right: 20px;
	border: 1px solid rgb(255,153,0);
	color: #333;
}
.activity-downlad a:hover{
	background: rgb(255,153,0);
	color: #fff;
}
#activity-upload .table-container .overflow{
	max-height: 1500px;
	overflow: auto;
}
/* 活动信息start */
.activity-info .time-range td:first-child,.time-range tr td:last-child{width:15%;text-align:center;}
.activity-info .time-range td{border-top:solid 1px #ccc;border-bottom:solid 1px #ccc;}
.activity-info .time-range td:first-child{border-left:solid 1px #ccc;}
.activity-info .time-range td:last-child{border-right:solid 1px #ccc;}
.activity-info .time-range thead tr{height:35px;}
.activity-info .time-range tbody tr{height:50px;}
.activity-info .btn-danger{width:auto;padding:10px 5px;}
.activity-info .btn-concle,.activity-info .btn-save{padding: 10px 47px;}
.activity-info .form-control[readonly]{background-color: #fff;}
.activity-info .input-daterange p.error{position: absolute;}
.activity-info .input-daterange span.error_tips{position: relative;right: -240px;top: 9px;color:#eb0038;}
.activity-info p.over_tips span.error_tips{display: inline-block;width: 230px;}
.activity-info .error_bor{border:solid 1px #eb0038;box-shadow: none;}
.activity-info .time_list{width:335px;overflow:auto;}
.activity-info .time_list .time{height:26px;line-height:26px;display:block;float:left;width:45px;color:#333;font-size:14px;text-align:center;}
.activity-info .ipt-vail{color:#eb0038;}
.activity-info .ipt-tips,.activity-info .ipt-vail,.radio-wrap{padding-top:7px;display:inline-block;}
.activity-info .dn{display:none;}
.activity-info .form_datetime{position:relative;}
.activity-info .add-on{display: inline-block;width:100%;height:34px;position:absolute;left:0;z-index:99;}
.activity-info .select-coupon{cursor:pointer; display: inline-block;height: 34px;line-height: 34px;border: solid 1px #d2d6de;width: 100%;padding: 0px 10px;}
.activity-info .select-coupon .icon-down_arrow1{float:right;margin-top:10px;}
.activity-info .coupon-par{position:relative;}
.activity-info .coupon-wrap{padding:20px 15px;border:solid 1px #d2d6de;position:absolute;left:-44%;z-index:9999;background:#fff;width:200%;top:0;display:none;}
.coupon-par.show .coupon-wrap{display:block;/* max-height: 200px;overflow-y: auto; */} 
.coupon-list{margin-bottom:20px;max-height: 100px;overflow-y: auto;}
.coupon-list .s-coupon{border:solid 1px #d2d6de;padding:5px;float:left;margin-right:10px;margin-bottom:10px;font-weight: normal;}
.coupon-list input[type="checkbox"]:checked+label{border-color:#b1191a;color:#b1191a;}
.coupon-list input[type="checkbox"]{display:none;}
.coupon-list:after{content:"";display:block;clear:both;}
.coupon-save,.coupon-cancel{cursor:pointer;padding: 5px 15px;border: solid 1px #d2d6de;margin-right:10px;}
.coupon-save{background:#c9302c;border-color:#d20032;color:#fff;}
.coupon-save:hover{background:#d20032;color:#fff;}
.datetimepicker-hours thead .prev,.datetimepicker-hours thead .next{visibility:hidden !important;}
.selected-list .s-selected{display:inline-block;margin: 0 10px 10px 0;padding: 0 9px 0 11px;height: 28px;line-height: 28px;border: 1px dashed #ddd;color: #333;position:relative;font-weight:normal;}
.selected-list .s-selected:hover{color:#b1191a;border-color:#b1191a;}
.selected-list .delete-btn:hover{cursor:pointer;}
.radio-wrap label{font-weight:400;margin: 0px 20px 0px 5px;}
.radio-wrap input[type="radio"],.upload-icon-wrap input[type="checkbox"]{position:relative;top:2px;}
.upload-icon-wrap label{font-weight:400;}
.icon-wrap{display:inline-block;width:100px;height:100px;border:solid 1px #ccc;position:relative;overflow:hidden;}
.icon-wrap:hover .upload-btn,.icon-wrap  span.show-btn{bottom:0px;}
.upload-icon-wrap .label-tlt{margin-right:10px;float:left;}
.upload-icon-wrap .line{margin-bottom:20px;}
.upload-icon-wrap{border:solid 1px #ccc;padding-top:15px;padding-bottom:15px;margin-left:15px;}
.check-wrap label{margin-left:5px;margin-right:20px}
.check-list{position:relative;top:-2px;}
.icon-wrap .upload-btn{
	display: block;
	width:100px;  
    padding: 8px 0px;
    text-align: center;
    color: #fff;
    background: rgba(0,0,0,0.3);
    position: absolute;
    cursor: pointer;
    bottom:-40px;
    }
.upload-icon-wrap:before{
	content: '';
    position: absolute;
    left:10px;
    top: -13px;
    border-width: 6px 6px 6px 6px;
    border-style: solid;
    border-color: transparent transparent #ccc transparent;
}
.select-supplier:before{
	left:95px;
}
.icon-wrap .add-icon{
	display: inline-block;
    width: 100px;
    height: 100px;
    font-size: 50px;
    text-align: center;
    line-height: 85px;
    color: #e4e4ea;
}
.delete-icon{
	cursor:pointer;
	position:relative;
	top:-5px;
	
}
.activity-detail .upload-icon-wrap{
	margin-left:5px;
}
.upload-icon-wrap span.check-wrap {
    display: inline-block;
    width: 105px;
    position: relative;
    top: -80px;
}

.glyphicon {
    position: relative;
    top: 1px;
    display: inline-block;
    font-family: 'Glyphicons Halflings' !important;
    font-style: normal;
    font-weight: 400;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.glyphicon.icon-arrow-right:before {
    content: "\e092";
}
.glyphicon.icon-arrow-left:before {
    content: "\e091";
}
/* 活动信息end */

/* 活动维护商品start */
.goods-maintain .time-tab li{cursor:pointer;}
.goods-maintain .box-footer .btn-danger{width:auto;padding:10px 54px;}
.goods-maintain .btn-concle{padding: 10px 47px;}
.goods-maintain .col-sm-4,.col-sm-9{padding-top:7px;}
.goods-maintain .form-horizontal .form-group{margin-left:0px;margin-right:0px;}
.goods-maintain .control-label{color:#666;}
.goods-maintain .commodity-edit textarea{width:450px;height:70px;resize:none;}
.goods-maintain .commodity-edit textarea,.commodity-edit input{border: solid 1px #d2d6de;}
.goods-maintain .img_upload{padding:0px;overflow:auto;}
.goods-maintain .img_upload li{width:150px;height:150px;float:left;margin-left:20px;border:solid 1px #ccc;list-style-type: none;overflow: hidden;position: relative;}
.goods-maintain .img_upload li:first-child{margin:0px;}
.goods-maintain .img_upload li:hover .i-delete{top:0px;}
.img_upload li .i-delete{position: absolute;right: 0px;top:-20px;font-size: 14px;padding: 4px;cursor:pointer;}
.img_upload li .i-delete:hover{background:#b1191a;color:#fff;}
.goods-maintain .commodity-edit .layui-layer-btn{text-align:center;}
.goods-maintain .commodity-edit .layui-layer-btn .layui-layer-btn0{border-color:#eb0038;background-color:#eb0038;}
.goods-maintain .prd-list{width:100%;}
.goods-maintain .prd-list thead tr{height:50px;}
.goods-maintain .prd-list td{border-top: solid 1px #f4f4f4;border-bottom: solid 1px #f4f4f4;text-align:center;}
.goods-maintain .prd-list td:first-child{border-left: solid 1px #f4f4f4;width:29%;padding-left:3%;text-align:left;position: relative;}
.goods-maintain .prd-list td:last-child{border-right: solid 1px #f4f4f4;}
.goods-maintain .prd-info{width:100%;}
.goods-maintain .prd-info img{float:left;width:80px;height:80px;margin:10px auto;}
.goods-maintain .prd-info .p_info{float:left;margin-top:12px;width:60%;margin-left:1%;}
.goods-maintain .prd-info .p_info p{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:12em;}
.goods-maintain .img_upload img{width:148px;height:148px;}
.goods-maintain .img_upload .upload_btn{display:block;width:150px;padding:10px 0px;text-align:center;color:#fff;background:rgba(0,0,0,0.3);position:absolute;cursor:pointer;}
.goods-maintain .img_upload li:hover .upload_btn{bottom:0px;}
.goods-maintain .db{display:block;text-align: left;}
.goods-maintain .prd-list td:nth-child(5) .db{width: 80px;margin-left: 45%;}
.goods-maintain .prd-list td:nth-child(6) .db{width: 115px;margin-left: 40%;}
.goods-maintain .error_bor{border:solid 1px #b1191a;}
.goods-maintain .commodity-edit .form-group{margin-left:0px;margin-right:0px;}
.goods-maintain .prd-list thead td{background:#f2f5fa;}
.goods-maintain .prd-area{max-height: 4811px;overflow-x: hidden;overflow-y: auto;}
.goods-maintain .mt30{margin-top:30px;}
.goods-maintain .prd-list td:nth-child(2),.prd-list td:nth-child(3),.prd-list td:nth-child(4),.prd-list td:last-child{width:8%;}
.goods-maintain .prd-list td:nth-child(5),.prd-list td:nth-child(6){width:15%;}
.goods-maintain .over-head{width:99%;padding-right:17px;}
.goods-maintain .commodity-edit{display:block;background:#fff;width:1100px;height:500px;overflow:auto;margin:0px auto;}
.layer-box{display:none;}
.layer-top{width:1100px;margin:0px auto;height:40px;line-height:40px;border-bottom: 1px solid #eee;background-color: #F8F8F8;padding-left:20px;}
.layer-top .icon-close-min{float:right;margin-top: 10px;margin-right: 10px;cursor: pointer;font-size:16px;}
.layer-box .layui-layer-btn{width:1100px;margin:0px auto;height:50px;background:#fff;}
.goods-maintain .check-span{position: absolute;left: 5px;top: 40%;}
.goods-maintain thead .check-span{top: 13px;}
.s_upload_prd .layui-layer-content{max-height:80%;}
.prd-list .cutstring{display:inline-block;width:7em;}
.selected-list.detail-list .s-selected {border-color:#b1191a;color:#b1191a;}
#useCoupon,.pd5{padding:5px;}
.input-tips{line-height:32px;}
.sel-supplier-btn{cursor:pointer;}
.suppliers-wrap{display:none;padding-left:20px;overflow-x: hidden;}
.a-supplier{margin-left:10px;color:#b1191a;max-width: 61%;display: inline-block;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;white-space: nowrap;position: relative;top: 4px;}
/* 活动维护商品end */