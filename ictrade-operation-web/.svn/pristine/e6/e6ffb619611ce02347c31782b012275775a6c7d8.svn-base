}
.content_div{
	border-bottom: 1px solid #e4e4e4;
    display: inline-block;
    height: auto;
    width: 97%;
    margin-top: 20px;
    margin-left: 15px;
    margin-right: -15px;
}
.content_div:last-child {
    border-bottom: none;
}
.returnBtn{
    margin-top: 20px;
    margin-bottom: 30px;
    padding-left: 50px;
}
.returnBtn a{
	color:#206FD9;
	cursor: pointer;
}
.returnBtn a:hover, .returnBtn a:active,.returnBtn a:focus {
    text-decoration: underline;
    color: #666666;
}
.reset_btn a:hover, .reset_btn a:active,.reset_btn a:focus {
    text-decoration: underline;
    color: #666666;
}

/*报错红框*/
.bord_error{border: 1px solid #c11f2e;}

/*验证样式*/
em[class*='error'], i[class*='error'] {
	/* margin-top: 25px; */
    position: absolute;
	margin-left: 20px;
    padding: 8px 12px;
    max-width: 190px;
    box-sizing: border-box;
    background-color: #fff4d7;
    font-size: 12px;
    color: #666;
}

em[for=corCategory] {
	left: 270px;
}

em[class*='error']:before, i[class*='error']:before {
    border: solid transparent;
    margin-left: -25px;
    width: 0;
    font-size: 19px;
    border-right-color: #fff4d7;
    color: #fff4d7;
}
.icon-confirm {
	/* position: absolute;
