@CHARSET "UTF-8";

.tree_area{position: absolute;padding: 5px;margin: 20px 0px;width: 15%;min-height: 565px;border: 1px solid #d2d2d2; max-height: 400px; overflow: auto; min-width: 180px;background: #fff;}
.h_content_area{padding: 5px;margin: 20px 0px;min-width: 780px;min-height: 565px;border: 1px solid #d2d2d2;margin-left: 19%;width: 81%;background: #fff;}
.new_btn{display:inline-block;border: solid 1px #b1191a;color: #b1191a !important;padding: 5px 15px;}

.s-help-maintain table td{text-align:center;}
.s-help-maintain .oper-btn,.s-help-maintain .edit_btn{margin:0px 5px;cursor:pointer;}
.s-help-maintain .new_save{margin-top:20px;border:solid 1px #f4f4f4;}
.s-help-maintain .new_save p{padding-left:20px;}
.s-help-maintain .module-tlt{background:#f8f8f8;height:30px;line-height:30px;padding-left:10px;color:#333;}
.s-help-maintain .new_save .form-group:after{
	content: '';
	clear: both;
	display:block;
}

.s-help-maintain .new_save .form-group{
	color: #333;
}