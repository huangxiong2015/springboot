.sub_btn:active {background-color: #b1191a;}	/* 选定的链接 */

/* 表单输入非法提醒样式 */
em[class*='error'],i[class*='error'] {
	position: absolute;
	left: 467px;
	top: 1px;
	display: block;
	padding: 9px 10px;
	max-width: 165px;
	box-sizing: border-box;
	background-color: #fff4d7;
	font-size: 12px;
	color: #666;  /*transition:top 0.3s;*/
}
em[class*='error']:before,i[class*='error']:before {
	border: solid transparent;
	content: ' ';
	height: 0;
	right: 100%;
	position: absolute;
	width: 0;
	border-width: 5px;
	border-right-color: #fff4d7;
	top: 60%;
	border-width: 8px;
	margin-top: -11px;
}
i[class*='error']{display : none;}
.icon-confirm {
	position: absolute;
	position: absolute;
	left: 467px;
	top: 1px;
	color: #87b848;
	font-size: 24px;
}

