.up_file{
	position: absolute;
	left: 0;
	top: 0;
	width: 80px;
	height: 35px;
	filter:alpha(opacity=0);
 	-moz-opacity:0.0;
 	-khtml-opacity: 0.0;
 	opacity: 0.0;
	cursor: pointer;
}
