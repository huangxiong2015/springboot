	color: #333;
	font-weight: normal;
}
.order_dt{
	font-size: 14px;
	color: #333;
}
.order_dt ul{
	float: 	left;	
	margin-top: 20px;
	margin-bottom: 20px;
}
.order_dt li{
	display: inline-block;
	*display: inline;
	*zoom: 1;
	margin-right: 28px;
}
.order_dt .txt{
	color: #666;
}
.order_dt .more_dt{
	position: relative;
	text-decoration: none;
	color: #3377aa;
}
/* 更多详情弹出框 */
.order_dt .more_box{
	display: none; 
	position: absolute;
	width: 346px;
	/* height: 120px; */
	/* padding-top: 20px; */
	padding-bottom: 20px;
	padding-left: 30px;
	left: 50%;
	margin-left: -188px;
	top: 30px;
	border: 1px solid #ddd;
	background-color: #fffef0;
	font-size: 14px;
	color: #666;
	z-index: 3;
}
.more_box ul{
	padding: 0;
	margin: 0;
}
.more_box li{
	padding-top: 15px;
}
.more_box p{
	height: 24px;
	line-height: 24px;
}
.more_box .g3{
	color: #333;
}
.more_dt:hover .more_box{
	display: block;
}
/* triangle */
.more_box .triangle {
	display: block;
	width: 0;
	height: 0;
	border-width: 10px;
	border-style: solid;
	border-color: transparent  transparent #ddd  transparent;
	position: absolute;
	top: -20px;
	left: 50%;
	margin-left: -10px;
}
 .more_box .triangle em {
	display: block;
	width: 0;
	height: 0;
	border-width: 10px;
	border-style: solid;
	border-color: transparent  transparent #fffef0 transparent;
	position: absolute;
	bottom: -12px;
	left: -10px;
} 

