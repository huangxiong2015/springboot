.lemon-steps-small .lemon-steps-item.lemon-steps-status-wait .lemon-steps-tail-right,.lemon-steps-small .lemon-steps-item.lemon-steps-status-process .lemon-steps-tail-right,.lemon-steps-small .lemon-steps-item.lemon-steps-status-error .lemon-steps-tail-right,.lemon-steps-small .lemon-steps-item.lemon-steps-status-finish  .lemon-steps-tail-right{
