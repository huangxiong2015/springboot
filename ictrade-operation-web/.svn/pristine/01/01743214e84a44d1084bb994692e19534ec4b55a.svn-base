/*lemon-alter*/

.lemon-alert {
    position: relative;
    padding: 8px 48px 8px 16px;
    border-radius: 6px;
    color: #495060;
    font-size: 12px;
    line-height: 16px;
    margin-bottom: 10px
}

.lemon-alert.lemon-alert-with-icon {
    padding: 8px 48px 8px 38px
}

.lemon-alert-icon {
    font-size: 14px;
    top: 8px;
    left: 16px;
    position: absolute
}

.lemon-alert-desc {
    font-size: 12px;
    color: #495060;
    line-height: 21px;
    display: none;
    text-align: justify
}

.lemon-alert-success {
    border: 1px solid #d1f2e1;
    background-color: #e8f9f0
}

.lemon-alert-success .lemon-alert-icon {
    color: #19be6b
}

.lemon-alert-info {
    border: 1px solid #d5e8fc;
    background-color: #eaf4fe
}

.lemon-alert-info .lemon-alert-icon {
    color: #2d8cf0
}

.lemon-alert-warning {
    border: 1px solid #dacd8f;
    background-color: #fff7d1
}

.lemon-alert-warning .lemon-alert-icon {
    color: #d79413
}

.lemon-alert-error {
    border: 1px solid #fbd9d0;
    background-color: #fdece8
}

.lemon-alert-error .lemon-alert-icon {
    color: #ed3f14
}

.lemon-alert-close {
    font-size: 12px;
    position: absolute;
    right: 16px;
    top: 8px;
    overflow: hidden;
    cursor: pointer
}

.lemon-alert-close .lemon-icon-ios-close-empty {
    font-size: 22px;
    color: #999;
    transition: color .2s ease;
    position: relative;
    top: -3px
}

.lemon-alert-close .lemon-icon-ios-close-empty:hover {
    color: #444
}

.lemon-alert-with-desc {
    padding: 16px;
    position: relative;
    border-radius: 6px;
    margin-bottom: 10px;
    color: #495060;
    line-height: 1.5
}

.lemon-alert-with-desc.lemon-alert-with-icon {
    padding: 16px 16px 16px 69px
}

.lemon-alert-with-desc .lemon-alert-desc {
    display: block
}

.lemon-alert-with-desc .lemon-alert-message {
    font-size: 14px;
    color: #1c2438;
    display: block
}

.lemon-alert-with-desc .lemon-alert-icon {
    top: 50%;
    left: 24px;
    margin-top: -21px;
    font-size: 28px
}

.lemon-alert-with-banner {
    border-radius: 0
}

