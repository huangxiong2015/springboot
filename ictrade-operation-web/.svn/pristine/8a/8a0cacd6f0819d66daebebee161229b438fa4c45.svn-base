.lemon-steps.green .lemon-steps .lemon-steps-item .lemon-steps-tail>i:after,
.lemon-steps.green .lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner{
     border-color: #87b848;
 }
.lemon-steps.green .lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner>.lemon-steps-icon,
.lemon-steps.green .lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner span ,
.lemon-steps.green .lemon-steps-item.lemon-steps-custom.lemon-steps-status-process .lemon-steps-head-inner>.lemon-steps-icon,
.lemon-steps.green .lemon-steps-item.lemon-steps-status-finish .lemon-steps-tail-right,
.lemon-steps.green .lemon-steps-item.lemon-steps-status-finish .lemon-steps-title,
