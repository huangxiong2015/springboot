/*步骤*/
.lemon-steps {
    font-size: 0;
    width: 100%;
    line-height: 1.5
}

.lemon-steps-item {
    display: inline-block;
    position: relative;
    vertical-align: top
}

.lemon-steps-item.lemon-steps-status-wait .lemon-steps-head-inner {
    background-color: #fff
}

.lemon-steps-item.lemon-steps-status-wait .lemon-steps-head-inner>.lemon-steps-icon,.lemon-steps-item.lemon-steps-status-wait .lemon-steps-head-inner span {
    color: #ccc
}

.lemon-steps-item.lemon-steps-status-wait .lemon-steps-content,.lemon-steps-item.lemon-steps-status-wait .lemon-steps-title {
    color: #999
}

.lemon-steps-item.lemon-steps-custom .lemon-steps-head-inner {
    background: 0 0;
    border: 0;
    width: auto;
    height: auto
}

.lemon-steps-item.lemon-steps-custom .lemon-steps-head-inner>.lemon-steps-icon {
    font-size: 20px;
    top: 2px;
    width: 20px;
    height: 20px
}

.lemon-steps-item.lemon-steps-custom.lemon-steps-status-process .lemon-steps-head-inner>.lemon-steps-icon {
    color: #2d8cf0
}

.lemon-steps-item:last-child .lemon-steps-tail, .lemon-steps-item:last-child .lemon-steps-tail-right {
    display: none
}

.lemon-steps .lemon-steps-head,.lemon-steps .lemon-steps-main {
    position: relative;
    display: inline-block;
    vertical-align: top
}
.lemon-steps .lemon-steps-head {
    background: #fff
}

.lemon-steps .lemon-steps-head-inner {
    display: block;
    width: 26px;
    height: 26px;
    line-height: 24px;
    margin-right: 8px;
    text-align: center;
    border: 1px solid #ccc;
    border-radius: 50%;
    font-size: 14px;
    transition: background-color .2s ease-in-out
}

.lemon-steps .lemon-steps-head-inner>.lemon-steps-icon {
    line-height: 1;
    position: relative
}

.lemon-steps .lemon-steps-head-inner>.lemon-steps-icon.ivu-icon {
    font-size: 24px
}

.lemon-steps .lemon-steps-head-inner>.-icon.ivu-icon-ios-checkmark-empty,.lemon-steps .lemon-steps-head-inner>.lemon-steps-icon.ivu-icon-ios-close-empty {
    font-weight: 700
}

.lemon-steps .lemon-steps-main {
    margin-top: 2.5px;
    display: inline;
    width: 200px;
}

.lemon-steps .lemon-steps-custom .lemon-steps-title {
    margin-top: 2.5px
}

.lemon-steps .lemon-steps-title {
    display: inline-block;
    margin-bottom: 4px;
    padding-right: 10px;
    font-size: 14px;
    font-weight: 700;
    color: #666;
    background: #fff
}

.lemon-steps .lemon-steps-title>a:first-child:last-child {
    color: #666
}

.lemon-steps .lemon-steps-item-last .lemon-steps-title {
    padding-right: 0;
    width: 100%
}

.lemon-steps .lemon-steps-content {
    font-size: 12px;
    color: #999
}

.lemon-steps .lemon-steps-tail {
    width: 100%;
    padding: 0 10px;
    position: absolute;
    left: 0;
    top: 13px
}

.lemon-steps .lemon-steps-tail>i {
    display: inline-block;
    width: 100%;
    /*height: 1px; background: #e9eaec;border-radius: 1px;*/
    vertical-align: top;
    position: relative;
    border-top: #e9eaec 1px solid;
}

.lemon-steps .lemon-steps-tail>i:after {
    content: "";
    width: 0;
    height: 100%;
    border-top: #e9eaec 1px solid;
    opacity: 0;
    position: absolute;
    top: -1px;
}
.lemon-steps-vertical .lemon-steps-item {
    display: block
}

.lemon-steps-vertical .lemon-steps-tail {
    position: absolute;
    left: 13px;
    top: 0;
    height: 100%;
    width: 1px;
    padding: 30px 0 4px
}

.lemon-steps-vertical .lemon-steps-tail>i {
    height: 100%;
    width: 1px;
    border-left: 1px solid #e9eaec;
}

.lemon-steps-vertical .lemon-steps-tail>i:after {
    height: 0;
    width: 100%;
    border-left: 1px solid #e9eaec;
}

.lemon-steps-vertical .lemon-steps-status-finish .lemon-steps-tail>i:after {
    height: 100%
}

.lemon-steps-vertical .lemon-steps-head {
    float: left
}

.lemon-steps-vertical .lemon-steps-head-inner {
    margin-right: 16px
}

.lemon-steps-vertical .lemon-steps-main {
    min-height: 47px;
    overflow: hidden;
    display: block
}

.lemon-steps-vertical .lemon-steps-main .lemon-steps-title {
    line-height: 26px
}

.lemon-steps-vertical .lemon-steps-main .lemon-steps-content {
    padding-bottom: 12px;
    padding-left: 0
}

.lemon-steps-vertical .lemon-steps-custom .lemon-steps-icon {
    left: 4px
}

.lemon-steps-horizontal.lemon-steps-hidden {
    visibility: hidden
}

.lemon-steps-horizontal .lemon-steps-item:not(:first-child) .lemon-steps-head {
    padding-left: 10px;
    margin-left: -10px
}

.lemon-steps-item.lemon-steps-status-wait .lemon-steps-tail>i {
    /* background-color: #e9eaec*/
    border-color: #e9eaec;
}

.lemon-steps-item.lemon-steps-status-process .lemon-steps-head-inner {
    border-color: #2d8cf0;
    background-color: #2d8cf0
}

.lemon-steps-item.lemon-steps-status-process .lemon-steps-head-inner>.lemon-steps-icon,.lemon-steps-item.lemon-steps-status-process .lemon-steps-head-inner span {
    color: #fff
}

.lemon-steps-item.lemon-steps-status-process .lemon-steps-content,.lemon-steps-item.lemon-steps-status-process .lemon-steps-title {
    color: #666
}

.lemon-steps-item.lemon-steps-status-process .lemon-steps-tail>i {
    /* background-color: #e9eaec*/
    border-color: #e9eaec;
}
.lemon-steps-item.lemon-steps-status-wait .lemon-steps-tail-right,.lemon-steps-item.lemon-steps-status-process .lemon-steps-tail-right,.lemon-steps-item.lemon-steps-status-finish  .lemon-steps-tail-right{
    font-size: 12px;
    position: absolute;
    right: 10px;
    top: -2px;
    padding-left: 5px;
    color:#e9eaec;
    background: #fff;
}
.lemon-steps-item.lemon-steps-status-wait .lemon-steps-tail-right,.lemon-steps-item.lemon-steps-status-process .lemon-steps-tail-right,.lemon-steps-item.lemon-steps-status-error .lemon-steps-tail-right, .lemon-steps-item.lemon-steps-status-finish  .lemon-steps-tail-right{
    position: absolute;
    right: 10px;
    padding-left: 5px;
    color:#e9eaec;
    background: #fff;
    top: 2px;
    font-size: 14px;
}

.lemon-steps-item.lemon-steps-status-finish  .lemon-steps-tail-right{
    color: #2d8cf0
}
.lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner {
    background-color: #fff;
    border-color: #2d8cf0
}

.lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner>.lemon-steps-icon,.lemon-steps-item.lemon-steps-status-finish .lemon-steps-head-inner span {
    color: #2d8cf0
}

.lemon-steps-item.lemon-steps-status-finish .lemon-steps-tail>i:after {
    width: 100%;
    /*background: #2d8cf0;*/
    transition: all .2s ease-in-out;
    opacity: 1;
    border-color: #2d8cf0;
}

.lemon-steps-item.lemon-steps-status-finish .lemon-steps-content,.lemon-steps-item.lemon-steps-status-finish .lemon-steps-title {
    color: #999
}

.lemon-steps-item.lemon-steps-status-error .lemon-steps-head-inner {
    background-color: #fff;
    border-color: #ed3f14
}

.lemon-steps-item.lemon-steps-status-error .lemon-steps-content,.lemon-steps-item.lemon-steps-status-error .lemon-steps-head-inner>.lemon-steps-icon,.lemon-steps-item.lemon-steps-status-error .lemon-steps-title {
    color: #ed3f14
}

.lemon-steps-item.lemon-steps-status-error .lemon-steps-tail>i {
    /* background-color: #e9eaec*/
    border-color:#e9eaec;
}

.lemon-steps-item.lemon-steps-next-error .lemon-steps-tail>i,.lemon-steps-item.lemon-steps-next-error .lemon-steps-tail>i:after {
    /*background-color: #ed3f14*/
    border-color:#ed3f14;
}

.lemon-steps.lemon-steps-small .lemon-steps-head-inner {
    width: 18px;
    height: 18px;
    line-height: 14px;
    margin-right: 10px;
    text-align: center;
