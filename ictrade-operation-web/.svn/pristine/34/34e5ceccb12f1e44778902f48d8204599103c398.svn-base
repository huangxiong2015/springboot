@charset "utf-8";
@import url(skins/skin-lemon.min.css);
@import url(lemon-AdminLTE.css);
@import url(custom.css);

/*模态窗组件*/

.modal-mask {
    position: fixed ;/*fixed*/
    z-index: 9998;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .5);
    display: table;
    -webkit-transition: opacity .3s ease;
    transition: opacity .3s ease;
}

.modal-leave {
    opacity: 0
}

.modal-wrapper {
    display: table-cell;
    vertical-align: middle;
}

.modal-container {
    margin: 0px auto;
    padding: 20px 30px;
    background-color: #fff;
    border: 4px solid #ccc;
    border-radius: 2px;
    -webkit-box-shadow: 0 2px 8px rgba(0, 0, 0, .33);
    box-shadow: 0 2px 8px rgba(0, 0, 0, .33);
    -webkit-transition: all .3s ease;
    transition: all .3s ease;
    font-family: Helvetica, Arial, sans-serif;
    width: 300px;
}

.modal-header {
    border: none;
    padding: 0;
}

.modal-header h3 {
    margin-top: 0;
    color: #666;
    font-size: 14px;
    font-weight: bold;
}

.modal-body {
    overflow: auto;
    /* margin: 20px 0;*/
}

.modal-default-button {
    float: right;
}

.close {
    float: right;
    font-size: 21px;
    font-weight: bold;
    line-height: 1;
    color: #666;
    text-shadow: 0 1px 0 #fff;
    filter: alpha(opacity=20);
    opacity: .2;
}

.close:hover,
.close:focus {
    color: #000;
    text-decoration: none;
    cursor: pointer;
    filter: alpha(opacity=50);
    opacity: .5;
}

button.close {
    -webkit-appearance: none;
    padding: 0;
    cursor: pointer;
    background: transparent;
    border: 0;
}

.modal-header .close {
    text-shadow: none;
    font-size: 26px;
    margin-top: -20px;
    width: 35px;
    height: 35px;
    margin-right: -30px;
    font-weight: normal;
    opacity: 1;
}

.modal-title {
    margin: -10px;
}

.modal-header .close:hover {
    font-weight: normal;
    color: #fff;
    background: #b1191a;
}

.modal-footer {
    padding: 0;
    border: none;
    text-align: center;
}

.modal-footer .btn+.btn {
    margin-left: 30px;
}


/*grid表格组件*/

.table,
.table * {
    margin: 0 auto;
    padding: 0;
}

.table {
    display: table;
    width: 100%;
    border-collapse: collapse;
    table-layout: fixed;
}

.table.mb20 {
    margin-bottom: 20px;
}

.table-thead {
    display: table-header-group;
}

.table-tbody {
    display: table-caption;
}

.table-tr {
    display: table-row;
    line-height: 30px;
}

.table-hover>.table-tr:hover {
    background-color: #f5f5f5;
}

.table tr th {
    display: table-cell;
    font-weight: bold;
    height: 100%;
    border: 1px solid #eee;
    text-align: center;
    vertical-align: middle;
    padding: 10px;
    background: #f9f9f9;
}

.table>tbody>tr>td,
.table>tbody>tr>th,
.table>tfoot>tr>td,
.table>tfoot>tr>th,
.table>thead>tr>td,
.table>thead>tr>th {
    display: table-cell;
    height: 45px;
    vertical-align: middle;
}

.table>tr .left {
    text-align: left;
}

.table>tr .center {
    text-align: center;
}

.table>tr .right {
    text-align: right;
}

.table tr td .cont,
.table tr th .cont {
    padding: 5px;
    display: block;
}

.bor-bottom {
    border-bottom: 1px solid #eee;
}

.table .table tr td {
    border: none;
    border-bottom: 1px solid #eee;
}

.table .table tr:last-child td {
    border-bottom: none;
}

.table tr td img {
    width: 80%;
}

.table tr td .table {
    background: transparent;
    table-layout: fixed;
}

.fixed-width .overflow {
    overflow-x: scroll;
}

.table>tbody>tr>td {
    padding: 0;
}

.table>tbody>tr>td .tabcon {
    padding: 8px;
    position: relative;
}

.table>tbody>tr>td .table {
    background: transparent;
    table-layout: fixed;
}

.table>tbody>tr>td .table tr:first-child td {
    border-top: none;
}

.wid5 {
    width: 5%;
}

.wid10 {
    width: 10%;
}

.wid20 {
    width: 20%;
}

.wid30 {
    width: 30%;
}

.minlogo img {
    width: 50px;
}


/*分页*/

.page-turn {
    margin-left: 10px;
}

.page-turn span {
    display: inline-block;
    line-height: 25px;
}

.page-turn i {
    font-style: normal;
    margin-left: 5px;
    margin-right: 5px;
}

.page-turn .page-input {
    width: 50px;
    margin-left: 10px;
    margin-right: 10px;
}

.page-turn .page-btn {
    margin-left: 10px;
    margin-top: -3px;
    line-height: 18px;
}

.pagination>.active>a,
.pagination>.active>a:focus,
.pagination>.active>a:hover {
    background-color: #ccc;
    border-color: #ccc;
}

.pagination {
    margin: 0;
}

.pagination>li>.page-input {
    float: left;
    width: 60px;
    padding: 6px 10px;
    border: 1px solid #ddd;
}

.pagination>li>span,
.pagination>li>span:focus,
.pagination>li>span:hover {
