em[class*='error'], i[class*='error'] {
	margin-top: 0px;
    position: absolute;
	margin-left: 20px;
    padding: 10px 12px 10px 12px;
    max-width: 272px;
    box-sizing: border-box;
    background-color: #fff4d7;
    font-size: 12px;
    color: #666;
}
em[class*='error']:before, i[class*='error']:before {
    border: solid transparent;
    margin-left: -25px;
    width: 0;
    font-size: 19px;
    border-right-color: #fff4d7;
    color: #fff4d7;
    top: 75%;
}

em[class*='error'] {
	position: absolute;
	top: 1px;
    left: 360px;
	display: inline-block;
	padding: 8px 10px;
	box-sizing: border-box;
	background-color: #fff4d7;
	font-size: 12px;
	color: #666;  /*transition:top 0.3s;*/
	margin-left:15px;
}
em[class*='error']:before {
	border: solid transparent;
	content: ' ';
	height: 0;
	right: 100%;
	position: absolute;
	width: 0;
	border-width: 5px;
	border-right-color: #fff4d7;
	top: 60%;
	border-width: 8px;
	margin-top: -11px;
}

