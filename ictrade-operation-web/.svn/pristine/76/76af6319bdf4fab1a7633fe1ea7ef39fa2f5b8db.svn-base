@charset "utf-8";
/* 选择供应商 */
.letter-select-wrap{
	padding-top:7px;
}
.letter-select{
	padding-left: 20px;
	padding-right:20px;
    overflow-x: hidden;
}
.letter-select-wrap .form-group{
	margin-left:0;
	margin-right:0;
}
.ml5{
	margin-left:5px;
}
.advertisement-edit{
	cursor:pointer;
}
/* 组件已选择样式 */
.selected-list .s-selected,.letter-select-wrap .s-selected{
    display: inline-block;
    margin: 0 10px 10px 0;
    padding: 0 9px 0 11px;
    height: 28px;
    line-height: 28px;
    border: 1px dashed #ddd;
    color: #333;
    position: relative;
    font-weight: normal;
    cursor:pointer;
}
.selected-list .s-selected .icon-close-min,.letter-select-wrap .s-selected .icon-close-min{
	margin-left:10px;
}
.selected-list .s-selected:hover,.letter-select-wrap .s-selected:hover{
    color: #b1191a;
    border-color: #b1191a;
}
.letter-select-wrap .search-box input{
	line-height:30px;
	height:30px;
}
.letter-select-wrap .pinyin-t span a{
	margin: 0 2px 5px;
	line-height:22px;
}