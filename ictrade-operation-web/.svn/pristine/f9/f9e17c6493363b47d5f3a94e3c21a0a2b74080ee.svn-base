body{   
    margin: 0;
    padding: 0;
    list-style:none; background-color: #f1f2f6; font-size:14px;	}
.w {
    width: 1200px;
    margin: 0 auto;
}
.dsn{ display:none}
.top_bg{ width: 100%; height:500px; }
.rel {position: relative;}
.top_bg .active-detail {
    color: #fff;
    width: 100px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background: rgba(0,0,0,0.14);
    border-radius: 5px;
    cursor: pointer;
    position: absolute;
    top: 50px;
    right: 50px;
}
.show-product-list {
    margin-top: 30px
}
.show-product-list {
    width: 100%;
    background-color: #f1f2f6;
    padding-top: 1px;
    margin-top: 76px
}
.product-list-box {
    background-color: #fff;
    width: 1200px;
    display: inline-block
}
.product-list-title {
    width: 1200px;
    height: 60px;
    margin: auto;
    background-color: #fff3f3
}

.product-list-title ul {
    width: 1200px;
    height: 60px;
    margin: auto;
    border-top: 2px solid #a3261c
}

.product-list-title li {
    color: #000;
    float: left;
    width: 171px;
    font-size: 16px;
    font-weight: inherit;
    line-height: 60px;
    text-align: center
}

.product-list-box ul {
    width: 1200px;
    margin: auto;
    border: 1px solid #e4f1ff;
    border-top: 0;
    overflow: hidden
}

.product-list-box li {
    color: #000;
    height: 60px;
    width: 1200px;
    margin: auto;
    font-size: 12px
}

.product-list-box li span {
    width: 171px;
    height: 60px;
    line-height: 60px;
    text-align: center;
    display: block;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 14px
}
.product-list-box .buy-button {
    color: #fff;
    width: 106px;
    height: 36px;
    line-height: 36px;
    border-radius: 4px;
    display: inline-block;
    background-color: #a3261c;
}
.product-list .product-model {
    width: 221px;
    padding-left: 10px
}

.product-list .product-num {
    width: 121px
}

.product-list .product-price {
    width: 161px
}

.product-list-box li .product-model a {
    color: #333
}

.product-list {
    padding-bottom: 35px
}

.product-list-box li:nth-child(2n) {
    background-color: #f8f8f8
}

.product-list-box li:hover {
    background-color: #fff3f3
}
.product-list-box li .old-price {
    color: #848484; 
    text-decoration: line-through;
}
.product-list-box li .new-price {
    color: #b2100d; 
}
#kkpager {
    float: none
}

#kkpager .pageBtnWrap {
    float: none;
    margin: auto;
    display: block;
    text-align: center
}

.product-list-top {
    width: 1200px;
    height: 110px
}

.list-top-title {
    width: 450px;
    height: 50px;
    line-height: 50px;
    margin: auto;
    top: 30px;
    position: relative;
    color: #b2100d;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
    background: url(../../images/active/titleBox.png?v=170919184825) no-repeat top
}

.list-top-title .title-text {
     text-align:center;
}

.active-sale-title .product-list-top {
    height: 100px
}
 

.activity-description {
    margin-top: 0;
    padding-top: 0
}

.activity-description-list {
    padding-top: 0;
    height: auto;
    width: 1200px;
    background-color: #fff
}

.activity-description .product-list-top {
    height: 100px
}

.activity-description .list-top-title .title-text {
    left: 192px
}

.active-product-img {
    width: 1200px;
    height: 200px;
    margin: auto;
    margin-top: 20px;
    background: url(../../images/active/willasProduct.png?v=170919184825) no-repeat top
}
#kkpager {
    margin: 20px 0
}

.pageBtnWrap span,.pageBtnWrap a,#kkpager_btn_go {
    height: 35px;
    line-height: 35px;
    padding: 0 14px
}

.pageBtnWrap span.disabled {
    height: 35px;
    line-height: 35px;
    padding: 0 14px
}

.infoTextAndGoPageBtnWrap {
    display: none
}

.pageBtnWrap .curr {
    color: #fff;
    background-color: #b2100d
}

#kkpager .pageBtnWrap .disabled {
    color: #bfbfbf;
    background: #f2f2f2;
    border: 1px solid #e4e4e4
}

.active_search {
    position: relative;
    top: -12px
}

.goods_search {
    background: 0;
    margin-top: 0;
    width: auto;
    height: auto;
    float: right;
    margin-right: 50px
}

.go_search {
    width: 250px;
    height: 34px;
    line-height: 34px;
    padding: 0 35px 0 20px;
    border: 1px solid #c3c3c3;
    border-radius: 20px;
    font-size: 14px
}

.go_search:focus {
    border: 1px solid #666
}

.search_btn {
    position: absolute;
    top: 8px;
    right: 10px;
    width: 20px;
    height: 20px;
    background:#eee url(../../images/index/sprite.png?v=170919184825);
    background-position: -285px -285px
}

.search_btn:hover {
    background-position: -425px -285px
}

.goods_search .predictive_box {
    top: 45px;
    width: 306px;
    left: -100px
}

.predictive_box ul li a {
    width: 220px
}

.activity-description-box p {
    font-size: 14px
}
.loding-icon {
    width: 100px;
    height: 100px;
    margin: 200px auto 200px auto;
    background: url(../../images/static/defaultimgload.gif?v=170919184825)no-repeat center
}
.activity-description {
    margin-top: 0;
    padding-top: 0;
    height: auto;
    padding-bottom: 65px;
    position: relative;
    background-color: #fff;
}
.activity-description-box p {
    font-size: 14px;
    color: #000;
    letter-spacing: 0;
    line-height: 40px;
}
.willas-coupons{ margin-top: 30px; margin-bottom: 30px;}
.go-coupons{ background: #b1191a;  height: 150px;}
.go-coupons .go-coupon{ float: left; width: 220px; color: #fff; text-align: center;padding-top: 20px;}
.go-coupons .disabled {  background-color: #f4b3b3;color: #fff;
    cursor: default;}
.go-coupons .btn-coupon {
    color: #fff;
    width: 142px;
    height: 36px;
    font-size: 16px;
    line-height: 36px;
    text-align: center;
    display: block;
    margin:15px auto 0;
    border-radius: 3px;}
.coupon-text {
    font-size: 24px;
    text-align: center;
}
.coupon-detail {
    width: 1200px;
    height: 150px;
}
.go-coupons li{ float: left; border-left: 1px dashed #fff; width: 14%;
    padding-left: 1%;
    padding-right: 1%; height: 150px; color: #fff;}
.coupon-detail-top{ padding-top: 40px;}
.coupon-detail-num{font-size: 40px;}
.coupon-detail-left {
    float: left;
    width: 56%;
    text-align: right;
}
.coupon-detail-title {
    font-size: 16px;
    color: #f8e71c;
}
.coupon-detail-right {
    float: left;
    padding-left: 10px; 
    padding-top: 5px;
}
.use-type {clear: both; text-align: center; border-top:1px dashed #fff; padding-top: 5px;}

.right-nav-bar {
    width: 134px;
    height: 425px;
    top: 60px;
    right: 20px;
    z-index: 100;
    position: absolute;
    transition: .3s;
    background: url(../../images/active/cateRightBar.png?v=170930164840) no-repeat top
}

.right-nav-bar .go-top {
    margin-top: 8px
}
 
.right-fix {
    position: fixed!important;
    top: 60px
}

.right-nav-box {
    width: 110px;
    margin: auto;
    margin-top: 60px;
    background:#C51D1D;
    padding:0  5px 10px;
    border-radius: 0 0 5px 5px; 
}

.right-nav-box li { 
    height: 32px;
    margin-top: 3px; 
}

.right-nav-box li:hover a {
    background-color: #fff5f7;
    color: #b11919
}

.right-nav-box li a {
    color: #fff;
    width: 100px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    display: inline-block;
    font-size: 16px;
    background-color: #d95a5a;
    overflow: hidden
}

.right-nav-box li .active {
    color: #b11919;
    background-color: #fff
}

.right-nav-bar .go-top {
    width: 100px;
    margin: auto;
    margin-top: 25px;
    color: #fff;
    text-align: center;
    font-size: 16px;
    cursor:pointer;
}


.right-nav-bar .icon-indicate_up {
    color: #fff;
    font-size: 14px
}

