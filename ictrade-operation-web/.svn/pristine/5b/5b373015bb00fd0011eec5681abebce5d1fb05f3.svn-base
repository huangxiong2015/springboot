.selector_box{padding:20px;}
.search_criteria a {
        font-size:12px;
        display: inline-block;
        width: 18px;
        height: 18px;
        line-height: 18px;
        border: 1px solid #e4e4e4;
        margin-right: 5px;
        text-align: center;
        float: left;
        margin-top: 3px;
        color: #666;
    }
    .search_criteria .active {
        color: #b1191a;
        border: 1px solid #b1191a;
        cursor: pointer;
    }
    .search_criteria .disable {
        color: #d6d6d6;
        cursor: default;
    }
    .supplier_list{
      overflow: auto;
      max-height: 110px;
      margin-top: 20px;
    }
    .supplier_list span {
        font-size:12px;
        display: inline-block;
        width: 150px;
        margin-right: 30px;
        margin-bottom: 13px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        float: left;
        height: 16px;
        line-height: 16px;
    }
  .supplier_list a.active {
        color: #b1191a;
    }
  .supplier_list .mr0{margin-right:0px;}
  .s_select_data{
    max-height: 320px;
  }
  .search_box {
    width:160px;
    height:24px;
    line-height:24px;
    padding-left:10px;
  }
  
  .logo-box img{ width:120px;}
  .logo-box #uploadBtn {width: 120px;}
  
